{
  "name": "my-qwik-empty-starter",
  "type": "module",
  "description": "App with Routing built-in ready to create your app",
  "engines": {
    "node": "20.9.0"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "scripts": {
    "dev:backend": "node --import ./custom-imports.mjs --watch src/backend/run-backend.ts | pino-pretty",
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "test.unit": "vitest components",
    "test.unit.ui": "vitest --ui components",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.2.15",
    "@builder.io/qwik-city": "^1.2.15",
    "@types/bcrypt": "^5.0.1",
    "@types/eslint": "^8.44.4",
    "@types/express": "^4.17.21",
    "@types/node": "^20.8.4",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitest/ui": "^0.34.6",
    "eslint": "^8.51.0",
    "eslint-plugin-qwik": "^1.2.15",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.2.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.2.2",
    "undici": "^5.26.0",
    "vite": "^4.5.1",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@yucacodes/ui-qwik": "^0.0.15",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^4.18.2",
    "pino": "^8.16.2",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.32.6",
    "short-uuid": "^4.2.2",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "sqlite3": "^5.1.7",
    "tsyringe": "^4.8.0"
  }
}
