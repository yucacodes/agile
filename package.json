{
  "name": "my-qwik-empty-starter",
  "type": "module",
  "description": "App with Routing built-in ready to create your app",
  "engines": {
    "node": "20.11.1"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "scripts": {
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/express/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "build:backend": "tsc -p ./tsconfig.server.json && tsc-alias -f -p ./tsconfig.server.json",
    "build:front": "qwik build",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "dev:backend": "yts src/backend/run-backend.ts | pino-pretty",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start:front": "node server/entry.express",
    "start": "vite --open --mode ssr",
    "start:backend": "node ./dist/backend/run-backend.js",
    "test.backend": "vitest backend",
    "test.unit": "vitest components",
    "test.unit.ui": "vitest --ui components",
    "watch:backend": "yts --watch src/backend/run-backend.ts | pino-pretty",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.2.15",
    "@builder.io/qwik-city": "^1.2.15",
    "@types/bcrypt": "^5.0.1",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.44.4",
    "@types/express": "^4.17.19",
    "@types/node": "^20.8.4",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitest/ui": "^0.34.6",
    "@yucacodes/ts": "0.0.4",
    "compression": "^1.7.4",
    "eslint": "^8.51.0",
    "eslint-plugin-qwik": "^1.2.15",
    "express": "4.18.2",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.2.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.2.2",
    "undici": "^5.26.0",
    "vite": "^4.5.1",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@yucacodes/es": "0.0.14",
    "@yucacodes/ui-qwik": "^0.0.16",
    "redis": "^4.6.13",
    "sharp": "^0.32.6",
    "socket.io-client": "^4.7.2",
    "sqlite3": "^5.1.7"
  }
}
